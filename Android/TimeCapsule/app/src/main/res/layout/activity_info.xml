<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    tools:context=".Info">

    <TextView
        android:id="@+id/info_name"
        android:layout_width="267dp"
        android:layout_height="74dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="64dp"
        android:fontFamily="@font/countryside"
        android:text="[Developers]"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/name1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="101dp"
        android:layout_marginRight="81dp" />

    <TextView
        android:id="@+id/name1"
        android:layout_width="229dp"
        android:layout_height="50dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="55dp"
        android:fontFamily="@font/countryside"
        android:text="PremKumar A"
        android:textAlignment="center"
        android:textColor="#910a76"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/name2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/info_name"
        android:layout_marginLeft="101dp"
        android:layout_marginRight="81dp" />

    <TextView
        android:id="@+id/name2"
        android:layout_width="229dp"
        android:layout_height="50dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="66dp"
        android:fontFamily="@font/countryside"
        android:text="Mukeshwar S"
        android:textAlignment="center"
        android:textColor="#910a76"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/name3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name1"
        android:layout_marginLeft="101dp"
        android:layout_marginRight="81dp" />

    <TextView
        android:id="@+id/name3"
        android:layout_width="229dp"
        android:layout_height="50dp"
        android:layout_marginStart="101dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="188dp"
        android:fontFamily="@font/countryside"
        android:text="DineshKumaar K"
        android:textAlignment="center"
        android:textColor="#910a76"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name2"
        android:layout_marginLeft="101dp"
        android:layout_marginRight="81dp" />

</androidx.constraintlayout.widget.ConstraintLayout>